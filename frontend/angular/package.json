{
  "name": "snippeter-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.conf.json",
    "start:prod-mode": "ng serve --prod --proxy-config ./proxy.conf.json",
    "build": "ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "zip": "cd dist/angular && bestzip ../build.zip *",
    "upload": "node upload-to-ec2.js",
    "build-deploy": "npm run build && npm run zip && npm run upload",
    "generate": "graphql-codegen",
    "postinstall": "ngcc",
    "analyze-bundle-size": "npx webpack-bundle-analyzer dist/angular/stats.json"
  },
